{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "alkoumi/laravel-hijri-date": "^1.0",
    "archtechx/enums": "^0.3.2",
    "carlos-meneses/laravel-mpdf": "^2.1",
    "doctrine/dbal": "^3.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "irazasyed/telegram-bot-sdk": "^3.14",
    "kalnoy/nestedset": "^6.0",
    "laracasts/flash": "^3.2",
    "laravel/framework": "^12.0",
    "laravel/tinker": "^2.9",
    "laravel/ui": "^4.5",
    "league/flysystem-aws-s3-v3": "^3.15",
    "maatwebsite/excel": "^3.1",
    "mpdf/mpdf": "^8.1",
    "orangehill/iseed": "^3.0",
    "php-coord/php-coord": "^5.2",
    "psr/simple-cache": "^1.0",
    "spatie/laravel-activitylog": "^4.7",
    "spatie/laravel-html": "^3.12",
    "spatie/laravel-permission": "^6.0",
    "yajra/laravel-datatables": "^12.0",
    "yajra/laravel-datatables-oracle": "^12.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.8",
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.24",
    "laravel/sail": "^1.26",
    "laravel/telescope": "^5.8",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0",
    "spatie/laravel-ignition": "^2.4"
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/Helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
